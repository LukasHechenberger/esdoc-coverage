{
  "source": "./src",
  "destination": "./docs/api",
  "plugins": [{
    "name": "esdoc-standard-plugin",
    "option": {
      "brand": {
        "title": "esdoc-coverage"
      },
      "accessor": {
        "access": ["public", "package", "private"]
      },
      "test": {
        "type": "mocha",
        "source": "./test/src"
      },
      "manual": {
        "changelog": ["./CHANGELOG.md"]
      }
    }
  }]
}
